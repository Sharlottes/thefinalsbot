{
  "name": "2024-joy",
  "version": "0.0.1",
  "description": "2024 PROJECT JOY",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec \"yarn build && yarn start\"",
    "prod": "cross-env NODE_ENV=production yarn build && yarn start",
    "start": "node --loader=extensionless/register ./dist/main.js",
    "build": "swc ./src -d dist --strip-leading-paths",
    "ts-check": "tsc --noEmit",
    "format": "prettier --cache --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Discord-Joy-Support/2024-joy.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Discord-Joy-Support/2024-joy/issues"
  },
  "homepage": "https://github.com/Discord-Joy-Support/2024-joy#readme",
  "dependencies": {
    "@discordx/importer": "^1.3.0",
    "@discordx/utilities": "^6.1.0",
    "discord.js": "^14.14.1",
    "discordx": "^11.9.0",
    "dotenv": "^16.4.4",
    "express": "^4.19.2",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.2",
    "@swc/helpers": "^0.5.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "cross-env": "^7.0.3",
    "extensionless": "^1.9.6",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
